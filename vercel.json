{
  "redirects": [
    {"source": "/lessons", "destination": "/s?type=lesson"},
    {"source": "/courses", "destination": "/s?type=course"},
    {"source": "/search", "destination": "/s"},
    {"source": "/instructors", "destination": "/s"},
    {"source": "/browse", "destination": "/s"},
    {"source": "/browse/:context", "destination": "/s"},
    {"source": "/browse/:context/:tag", "destination": "/s/:tag"},
    {
      "source": "/courses/:title/:all*",
      "destination": "https://app.egghead.io/courses/:title/:all*"
    },
    {
      "source": "/users/:all*",
      "destination": "https://app.egghead.io/users/:all*"
    },
    {
      "source": "/admin/:all*",
      "destination": "https://app.egghead.io/admin/:all*"
    }
  ]
}
